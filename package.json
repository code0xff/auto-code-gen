{
  "name": "auto-code-gen",
  "version": "1.0.0",
  "description": "Automatic Code Generator",
  "main": "main.js",
  "scripts": {
    "start": "webpack --mode development&&electron .",
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "gen.code.auto",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "directories": {
      "output": "build/",
      "app": "."
    }
  },
  "author": "code0xff",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
